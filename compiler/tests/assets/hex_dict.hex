PROVINCES = {
  'BC': 'British Columbia',
  'AB': 'Alberta',
  'SK': 'Saskatchewan',
  'MB': 'Manitoba',
  'ON': 'Ontario',
  'QC': 'Quebec',
  'NB': 'New Brunswick',
  'NL': 'Newfoundland and Labrador',
  'PE': 'Prince Edward Isand',
  'NS': 'Nova Scotia'
};


FUNCS = {
  hello_world: (*args) -> print('Hello World'),
  square: (x) -> x*x,
  binary_search: (list, val, min, max) => {
    return -1 if max < min;
    mid = (min + max) / 2;
    if list[mid] > val {
      return self(list, val, min, mid-1);
    } elif list[mid] < val {
      return self(list, val, mid+1, max);
    } else {
      return mid;
    }
  }
};


SHORT_PROVINCES = {k,v for k,v in PROVINCES if len(v) <= 10};
